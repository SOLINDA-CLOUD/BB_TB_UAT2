<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_taboo" model="ir.sequence">
            <field name="name">Sequence PT Taboo</field>
            <field name="code">purchase.request.seq</field>
            <field name="padding">5</field>
            <field name="prefix">SDV/%(day)s/%(month)s%(y)s/</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_bamboo" model="ir.sequence">
            <field name="name">Sequence PT Bamboo</field>
            <field name="code">purchase.request.seq</field>
            <field name="padding">5</field>
            <field name="prefix">SDV/%(day)s/%(month)s%(y)s/</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_po_taboo" model="ir.sequence">
            <field name="name">Seq Purchase Order PT Taboo</field>
            <field name="code">purchase.order.seq</field>
            <field name="padding">5</field>
            <field name="prefix">PO/%(day)s/%(month)s%(y)s/</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_po_bamboo" model="ir.sequence">
            <field name="name">Seq Purchase Order PT Bamboo</field>
            <field name="code">purchase.order.seq</field>
            <field name="padding">5</field>
            <field name="prefix">PO/%(day)s/%(month)s%(y)s/</field>
            <field name="company_id" eval="False"/>
        </record>



    </data>
</odoo>